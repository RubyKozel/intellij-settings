<keymap version="1" name="Default copy" parent="$default">
  <action id="$Redo">
    <keyboard-shortcut first-keystroke="shift ctrl z" />
    <keyboard-shortcut first-keystroke="shift alt back_space" />
    <keyboard-shortcut first-keystroke="ctrl y" />
  </action>
  <action id="Console.TableResult.DeleteRows" />
  <action id="Editor Redo">
    <keyboard-shortcut first-keystroke="ctrl y" />
  </action>
  <action id="EditorDeleteLine" />
  <action id="QuickChangeScheme" />
  <action id="RenameElement">
    <keyboard-shortcut first-keystroke="shift f6" />
    <keyboard-shortcut first-keystroke="shift ctrl alt r" />
  </action>
  <action id="ShowIntentionActions">
    <keyboard-shortcut first-keystroke="alt enter" />
    <mouse-shortcut keystroke="button2" />
  </action>
  <action id="Switcher">
    <keyboard-shortcut first-keystroke="ctrl tab" />
    <keyboard-shortcut first-keystroke="shift ctrl tab" />
    <keyboard-shortcut first-keystroke="ctrl back_quote" />
  </action>
</keymap>